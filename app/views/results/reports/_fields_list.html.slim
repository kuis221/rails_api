#report-fields.field-list
  input type="search" id="field-search-input" name="field_search" title="Field Search" placeholder="Search for fields"
  - available_field_list.each do |group_name, items|
    span.group-name data-group="#{group_name}"= group_name
    ul.draggable-list
      - items.each do |id, name|
        li.report-field id="#{id}" data-field-id="#{id}" data-group="#{group_name}"
          a.field-remove-btn.icon-remove href="#" title="Remove"
          .field-label= name
          i.field-settings-btn.icon-caret-down
          .clearfix

